[TYPECHECK]

# List of members which are set dynamically and missed by pylint inference
# system, and so shouldn't trigger E1101 when accessed. Python regular
# expressions are accepted.
generated-members=numpy.*,torch.*

[FORMAT]
good-names=i,j,x,y,f,k,n,m,a,b,z,h,t,w,wc,f1,v,l

# let constants be named like variables
# loosely - only annoyed by this in scripts
# but don't want to add exceptions all the time
const-rgx=[A-Za-z_][A-Za-z0-9_]{2,30}$
